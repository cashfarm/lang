{
	"name": "@tokilabs/lang",
	"version": "1.0.1",
	"description": "Extends TypeScript/Javascript with basic classes and functions",
	"main": "lib/index.js",
	"private": false,
	"typings": "lib/index.d.ts",
	"scripts": {
		"clean": "rimraf ./lib",
		"docs": "yarn docs:html",
		"docs:html": "typedoc src/index.ts  --theme minimal --out docs",
		"docs:json": "typedoc  --json docs/typedoc.json src/index.ts",
		"docs:publish": "yarn docs:html && gh-pages -d docs",
		"lint": "eslint src/**/*.ts",
		"test": "jest",
		"test:watch": "jest --watch",
		"cover": "jest --coverage",
		"tsc": "tsc",
		"build": "yarn clean && yarn lint && yarn test && yarn tsc"
	},
	"engines": {
		"node": ">=14.17.0"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/tokilabs/lang.git"
	},
	"keywords": [
		"helpers",
		"FQN",
		"serialization",
		"uuid",
		"guid",
		"util",
		"server",
		"client",
		"browser",
		"decorators",
		"typescript",
		"functional"
	],
	"author": "Saulo Vallory <saulo@toki.life>",
	"contributors": [
		{
			"name": "Yago Fernandes",
			"email": "yago@toki.life"
		}
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/tokilabs/lang/issues"
	},
	"homepage": "https://github.com/tokilabs/lang#README",
	"dependencies": {
		"class-transformer": "^0.3.1",
		"nanoid": "^3.1.23",
		"prettier-eslint": "^12.0.0",
		"reflect-metadata": "^0.1.13",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@types/jest": "^26.0.23",
		"@types/node": "8",
		"@types/rimraf": "^3.0.0",
		"@types/source-map-support": "^0.5.3",
		"@types/uuid": "^8.3.0",
		"@typescript-eslint/eslint-plugin": "^4.25.0",
		"@typescript-eslint/parser": "^4.25.0",
		"eslint": "^7.27.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.23.3",
		"eslint-plugin-prettier": "^3.4.0",
		"gh-pages": "^1.0.0",
		"jest": "^27.0.3",
		"prettier": "^2.3.0",
		"rimraf": "^3.0.2",
		"source-map-support": "^0.5.19",
		"ts-jest": "^27.0.1",
		"ts-node": "^10.0.0",
		"ts-package-lint": "^0.2.2",
		"tsconfig-paths": "^2.3.0",
		"tslib": "^1.7.1",
		"typedoc": "^0.20.36",
		"typescript": "^4.3.2",
		"typings": "^1.3.2"
	}
}
